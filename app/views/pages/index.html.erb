<p style="color: green"><%= notice %></p>

<h1>Pages</h1>

<div id="pages">
  <% @pages.each do |page| %>
    <%= render page %>
    <p>
      <%= link_to "Show this page", [@book, page] %>
    </p>
  <% end %>
</div>

<%= link_to "New page", new_book_page_path(@book) %> |
<%= link_to "Back To Book", @book %>
